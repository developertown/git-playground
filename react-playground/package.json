    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@bigtest/interactor": "0.9.2",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.0",
    "@bigtest/react": "^0.1.1",
